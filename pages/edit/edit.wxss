/* pages/edit/edit.wxss */
@import "../common_style.wxss";


.submit-btn {
  width: 320rpx;
  min-height: 30rpx;
  margin: 15rpx;
  font-size: 30rpx;
  text-align: center;
  border: 1px #ddd;
}

.submit {
  background-color: #4aa8df;
  color: #eeeeee;
}

.submit_hover {
  background-color: #4aa8df;
  opacity: 0.5;
}

.background {
  width: 100%;
  height: 100%;
  position: fixed;
  background-size: 100% 100%;
  z-index: -1;
}